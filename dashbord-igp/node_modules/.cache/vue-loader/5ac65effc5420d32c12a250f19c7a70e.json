    <div class=\"row\">\r\n                                    <div class=\"col-md-6\">\r\n                                        <div class=\"form-group\">\r\n                                            <label>Libelle *</label>\r\n                                            <input type=\"text\" v-model=\"carType.libelle\" class=\"form-control\" placeholder=\"Entrer libelle\" required>\r\n                                        </div>\r\n                                    </div>\r\n                                    <div class=\"col-md-6\">\r\n                                        <div class=\"form-group\">\r\n                                            <label>Categorie parent *</label>\r\n                                            <select class=\"form-control\" v-model=\"carType.parent_id\" name=\"\" id=\"\">\r\n                                                <option v-for=\"cat in categories\" :key=\"cat.id\" :value=\"cat.id\"> {{cat.libelle}}  </option>\r\n                                            </select>\r\n                                            <div class=\"help-block with-errors\"></div>\r\n                                        </div>\r\n                                    </div>  \r\n                                    <div class=\"col-md-12\">\r\n                                        <div class=\"form-group\">\r\n                                            <label>Description *</label>\r\n                                            <input type=\"text\" v-model=\"carType.description\" class=\"form-control\" placeholder=\"Entrer description\" required>\r\n                                            <div class=\"help-block with-errors\"></div>\r\n                                        </div>\r\n                                    </div>                                 \r\n                                </div>                            \r\n                                <button type=\"submit\" class=\"btn btn-primary mr-2\">Enregistrer</button>\r\n                                <button type=\"reset\" class=\"btn btn-danger\">reinitialiser</button>\r\n                            </form>\r\n                        </div>\r\n                    </div>\r\n                    <div class=\"vld-parent\">\r\n                        <loading :active.sync=\"isLoading\" \r\n                        :can-cancel=\"true\" \r\n                        :is-full-page=\"fullPage\"></loading>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n            <!-- Page end  -->\r\n        </div>\r\n    </div>\r\n</template>\r\n<script>\r\nimport axios from \"axios\";\r\nimport {URL_TRANSPORT_API} from '@/config'\r\nimport Loading from 'vue-loading-overlay';\r\nimport 'vue-loading-overlay/dist/vue-loading.css';\r\nimport Swal from 'sweetalert2'\r\nexport default {\r\n    components: {\r\n            Loading\r\n    },\r\n    data() {\r\n        return {\r\n            carType:{},\r\n            categories:[],\r\n            isLoading: false,\r\n            fullPage: true\r\n        };\r\n    },\r\n    mounted(){\r\n        this.getCarType()\r\n    },\r\n    methods:{\r\n\r\n        store() {\r\n            this.isLoading = true\r\n            axios.post(URL_TRANSPORT_API+'car-types', this.carType)\r\n            .then(response => {\r\n                console.log(response.data)\r\n                this.carType = response.data\r\n                this.isLoading = false\r\n                if (response.data.state) {\r\n                    Swal.fire(\r\n                        'Effectué!',\r\n                        'La categorie a été ajoutée avec succès.',\r\n                        'success'\r\n                    )\r\n                    this.$router.push('/transport-car-types')\r\n                }\r\n            })\r\n            .catch(error => {\r\n                console.log(error)\r\n                this.errored = true\r\n                this.isLoading = false\r\n            })\r\n        },\r\n        getCarType() {\r\n            this.isLoading = true\r\n            axios.get(URL_TRANSPORT_API+'car-types')\r\n            .then(response => {\r\n                console.log(response.data)\r\n                this.categories = response.data\r\n                this.isLoading = false\r\n               \r\n            })\r\n            .catch(error => {\r\n                console.log(error)\r\n                this.errored = true\r\n                this.isLoading = false\r\n            })\r\n        },\r\n    }\r\n}\r\n</script>"]}]}